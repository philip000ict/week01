<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <div id ='panel'>
    <h1><%= title %></h1>
    <p>Welcome to <%= subheading %></p>
    <table>
      <% items.forEach((item) => { %>
        <tr>
          <td>
            <%= item.id %>
          </td>
          <td>
            <%= item.title %>
          </td>
          <td>
            <%= item.description %>
          </td>
        </tr>
      <% }); %>
    </table>
  </div>
  <footer><a <%= url %> > <%= link %> </a></footer>
  </body>
</html>
